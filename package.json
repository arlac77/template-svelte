{
  "keywords": [
    "svelte"
  ],
  "devDependencies": {
    "svelte": "^3.50.1",
    "@sveltejs/vite-plugin-svelte": "^1.0.6",
    "rollup-plugin-svelte": "--delete--",
    "rollup-plugin-postcss": "--delete--",
    "postcss-import": "--delete--",
    "postcss": "--delete--"
  },
  "template": {
    "inheritFrom": [
      "arlac77/template-vite",
      "arlac77/template-github-action"
    ],
    "mergers": [
      {
        "type": "Skip",
        "pattern": ".github/workflows/ci.yml"
      },
      {
        "type": "Readme",
        "pattern": "README.md",
        "options": {
          "badges": [
            {
              "name": "Svelte v3",
              "icon": "https://img.shields.io/badge/svelte-v3-orange.svg",
              "url": "https://svelte.dev",
              "order": 0.05
            }
          ]
        }
      }
    ],
    "usedBy": [
      "arlac77/template-svelte-app#next",
      "arlac77/template-svelte-component#next"
    ]
  }
}
