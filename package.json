{
  "keywords": [
    "svelte"
  ],
  "devDependencies": {
    "svelte": "^4.2.16",
    "@sveltejs/vite-plugin-svelte": "^3.1.0"
  },
  "template": {
    "inheritFrom": [
      "arlac77/template-vite",
      "arlac77/template-javascript",
      "arlac77/template-github-action"
    ],
    "mergers": [
      {
        "type": "Readme",
        "pattern": "README.md",
        "options": {
          "badges": [
            {
              "name": "Svelte v4",
              "icon": "https://img.shields.io/badge/svelte-v4-orange.svg",
              "url": "https://svelte.dev",
              "order": 0.05
            }
          ]
        }
      }
    ],
    "usedBy": [
      "arlac77/template-svelte-app",
      "arlac77/template-svelte-component"
    ]
  }
}
