{
  "keywords": [
    "svelte"
  ],
  "devDependencies": {
    "svelte": "^5.8.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.1"
  },
  "template": {
    "properties": {
      "typescript" : {
        "libs" : "--lib es2024 --lib dom "
      }
    },
    "inheritFrom": [
      "arlac77/template-vite",
      "arlac77/template-javascript",
      "arlac77/template-github-action"
    ],
    "mergers": [
      {
        "type": "Readme",
        "pattern": "README.md",
        "options": {
          "badges": [
            {
              "name": "Svelte v5",
              "icon": "https://img.shields.io/badge/svelte-v5-orange.svg",
              "url": "https://svelte.dev",
              "order": 0.05
            }
          ]
        }
      }
    ],
    "usedBy": [
      "arlac77/template-svelte-app",
      "arlac77/template-svelte-component"
    ]
  }
}
