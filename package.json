{
  "keywords": [
    "svelte"
  ],
  "devDependencies": {
    "rollup-plugin-svelte": "^7.0.0",
    "svelte": "^3.31.0",
    "rollup-plugin-postcss": "^4.0.0",
    "postcss-import": "^14.0.0",
    "postcss": "^8.2.2"
  },
  "template": {
    "inheritFrom": [
      "arlac77/template-rollup",
      "arlac77/template-github-action"
    ],
    "mergers": [
      {
        "type": "Skip",
        "pattern": ".github/workflows/ci.yml"
      },
      {
        "type": "Readme",
        "pattern": "README.md",
        "options": {
          "badges": [
            {
              "name": "Svelte v3",
              "icon": "https://img.shields.io/badge/svelte-v3-orange.svg",
              "url": "https://svelte.dev",
              "order": 0.05
            }
          ]
        }
      }
    ],
    "usedBy": [
      "arlac77/template-svelte-app",
      "arlac77/template-svelte-component"
    ]
  }
}